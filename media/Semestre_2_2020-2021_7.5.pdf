    var app3 = new Vue({

        el: '#app3',
        data: {
            count_globale: 0,
        },
        methods: {

            get_demandes_restantes: function () {
                axios.get('http://e-services.fsjes.uca.ma/student/get_notifications')
                    .then(response => {
                        $(".li_peu_delete").remove();
                        var count_g = 0;
                        // console.log(count_globale)
                        for (i = 0; i < response.data.length; i++) {
                            count_g = count_g + response.data[i]['total'];
                            $('#notifications_container').append('<li class="li_peu_delete">\n' +
                                '                    <a href="/student/demande_status/' + response.data[i][['demande_id']] + '">\n' +
                                '                      <i class="fa fa-bell"></i> ' + ' Une nouvelle action de ' + response.data[i]['label'] + "\n" +
                                '                       <h6>' + response.data[i]['description'] + '\n</h6>' +
                                '                    </a>\n' +
                                '                  </li>');
                        }
                        this.count_globale = count_g;
                        console.log(this.count_globale)
                        if (this.count_globale > 0) {
                            $('#count_globale').show();
                        }
                    })
                    .catch(error => {
                    })
            },
        },

        mounted: function () {
            this.get_demandes_restantes();
